CREATE TABLE SQLReqion (
  parentSportId INTEGER NOT NULL,
  idOpptyRegion INTEGER NOT NULL,
  opptyRegionName TEXT NOT NULL,
  opttyRegionCode TEXT NOT NULL,
  regionOrder INTEGER NOT NULL
);

selectAll:
SELECT * FROM SQLReqion;

insert:
INSERT INTO SQLReqion VALUES ?;

deleteItem:
DELETE FROM SQLReqion WHERE idOpptyRegion = ?;

deleteAllItems:
DELETE FROM SQLReqion;